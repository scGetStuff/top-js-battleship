    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run js",
            "program": "${file}",
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        },
        {
            "name": "Chrome Live Server",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:5500/dist",
            "webRoot": "${workspaceFolder}"
        }
    ]
}
